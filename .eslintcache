erComponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/FooterComponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Header.js",["166"],"import React, { useState } from \"react\";\nimport Nav from \"./Nav\";\nimport Authentication from \"./Authentification\";\nimport Logout from \"./Logout\";\nimport { Link } from \"react-router-dom\";\n\nconst Header = ({ token, setToken }) => {\n\n  if (!token) {\n    return (\n      <div className=\"NavWrapper\">\n        <Link to=\"/\">\n          <h1 className=\"custom-logo-font\">booksTrain</h1>\n        </Link>\n        <Authentication />\n      </div>\n    );\n  }\n  return (\n    <div className=\"NavWrapper\">\n      <Link to=\"/\">\n        <h1 className=\"custom-logo-font\">booksTrain</h1>\n      </Link>\n      <Nav />\n      <Logout token={ token } setToken={ setToken }/>\n    </div>\n  );\n};\n\nexport default Header;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Footercomponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/MainComponents/User.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/MainComponents/LoginForm.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/User.js",[],["167","168"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Loginform.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Homepage.js",["169","170"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Registrationform.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/test.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Testcomp.js",[],["171","172"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Logout.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Books.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Newshomepage.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchengine.js",["173"],"import React, { useEffect, useState } from \"react\";\nimport { Form, Card, Button } from \"react-bootstrap\";\n\nasync function searchRequest(credentials) {\n  return fetch(\"http://localhost:3002/booksSearch\", {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(credentials),\n  }).then((response) => response.json());\n}\n\nconst Searchengine = ({ setResponse, userId }) => {\n  useEffect(() => {\n    searchRequest();\n  }, []);\n\n  const [Author, setAuthor] = useState();\n  const [Genre, setGenre] = useState();\n  const [Country, setCountry] = useState();\n  const [BookName, setBookName] = useState();\n\n  const toLowerCase = (variable) => {\n    for (let item in variable) {\n      if (variable[item]) {\n        variable[item] = variable[item].toLowerCase();\n      }\n    }\n  };\n\n  const data = {\n    searchAuthor: Author,\n    searchGenre: Genre,\n    searchCountry: Country,\n    searchBookName: BookName,\n    userId: userId.toString(),\n  };\n\n  toLowerCase(data);\n\n  const checkEmptyInputs = (data, passPermit) => {\n    let cnt = 0;\n    for (let item in data) {\n      if (!data[item] || data[item] == \"\") {\n        cnt++;\n      }\n    }\n    return cnt > 3 ? (passPermit = false) : (passPermit = true);\n  };\n\n  const searchHandler = async (evt) => {\n    const requestPermit = false;\n    evt.preventDefault();\n\n    if (checkEmptyInputs(data, requestPermit)) {\n      const serverResponse = await searchRequest(data);\n      setResponse(serverResponse);\n    } else {\n      alert(\"Please fill out at least one input line (whatever you want)\");\n    }\n  };\n\n  return (\n    <Card className=\"text-left\">\n      <Card.Body>\n        <Form\n          className=\"text-secondary search-form-settings\"\n          onSubmit={searchHandler}\n        >\n          <Form.Group id=\"Author\">\n            <Form.Label>Search by Author</Form.Label>\n            <Form.Control\n              type=\"Author\"\n              onChange={(evt) => setAuthor(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Genre\">\n            <Form.Label>Search by Genre</Form.Label>\n            <Form.Control\n              type=\"Genre\"\n              onChange={(evt) => setGenre(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Country\">\n            <Form.Label>Search by Country</Form.Label>\n            <Form.Control\n              type=\"Country\"\n              onChange={(evt) => setCountry(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"bookName\">\n            <Form.Label>Search by book's name</Form.Label>\n            <Form.Control\n              type=\"bookName\"\n              onChange={(evt) => setBookName(evt.target.value)}\n            />\n          </Form.Group>\n          <Button\n            className=\"w-50 btn-secondary search-button-settings\"\n            type=\"submit\"\n          >\n            Search\n          </Button>\n        </Form>\n      </Card.Body>\n    </Card>\n  );\n};\n\nexport default Searchengine;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Mybooks.js",["174"],"import React, { useEffect, useState } from \"react\";\nimport Userbook from \"./Userbook\";\nimport Recsys from \"./Recsys\";\nimport Emptycurrentlist from \"./Emptycurrentlist\";\n\nconst Mybooks = ({ userData }) => {\n  const [myBooks, setMyBooks] = useState([]);\n\n  useEffect(() => {\n    getBooksList();\n  }, []); \n\n  const credentials = {\n    userId: userData.id,\n  };\n  const getBooksList = async () => {\n    const response = await fetch(\"http://localhost:3002/userbooks\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(credentials),\n    });\n    const data = await response.json();\n    setMyBooks(data);\n  };\n\n  if (myBooks.length > 0) {\n    return ( \n      <div>\n        <h3 className=\"text-secondary py-2\">\n          {userData.first_name} this is your current books list\n        </h3>\n        <ul className=\"list-unstyled border-bottom border-secondary\">\n          {myBooks.map((item) => (\n            <Userbook\n              key={item.book_id}\n              bookName={item.book_name}\n              bookAuthor={item.author}\n              date={item.start_date}\n              bookId={item.book_id}\n              userId={userData.id}\n              getBooksList={getBooksList}\n            />\n          ))}\n        </ul>\n        <div>\n          <Recsys userBooksData={myBooks} userData={userData} />\n        </div>\n      </div>\n    );\n  }\n  return <Emptycurrentlist userName={userData.first_name} />;\n};\n\nexport default Mybooks;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Clock.js",["175"],"import React, { useState, useEffect } from \"react\";\n\nconst Clock = () => {\n  const [currentTime, setCurrentTime] = useState(\n    new Date().toLocaleTimeString()\n  );\n\n  useEffect(() => {\n    const timer = setTimeout(() => {\n      setCurrentTime(new Date().toLocaleTimeString());\n    }, 1000);\n  });\n  return (\n    <div className=\"text-center text-secondary pt-1\">\n      <h6>{currentTime}</h6>\n    </div>\n  );\n};\n\nexport default Clock;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchresult.js",["176"],"import React from \"react\";\nimport Searchresultitem from \"./Searchresultitem\";\nimport { Button } from \"react-bootstrap\";\n\n\nconst Searchresult = ({ searchResult, setResponse, userName, userId }) => {\n  const returnHandler = () => {\n    setResponse(false);\n  };\n\n  const capitalizaResult = (data) => {\n    const titleCase = (string) => {\n      let splitStr = string.split(\" \");\n      let newArr = [];\n      for (let word of splitStr) {\n        word = word.charAt(0).toUpperCase() + word.slice(1);\n        newArr.push(word);\n      }\n      return newArr.join(\" \");\n    };\n\n    data.map((item) => {\n      let tmp = item.book_name.split(\" \");\n      tmp[0] = tmp[0][0].toUpperCase() + tmp[0].slice(1);\n      item.book_name = tmp.join(\" \");\n\n      item.author = titleCase(item.author);\n    });\n  };\n\n  capitalizaResult(searchResult);\n\n  return (\n    <div>\n      <ul className=\"list-unstyled\">\n        {searchResult.map((item) => (\n          <Searchresultitem\n            key={item.book_id}\n            bookName={item.book_name}\n            bookAuthor={item.author}\n            userName={userName}\n            userId={userId}\n            bookId={item.book_id}\n            bookStatus={item.user_id}\n            wishStatus={item.isInWishlist}\n          />\n        ))}\n      </ul>\n      <div className=\"text-right\">\n        <Button\n          onClick={returnHandler}\n          className=\"btn-secondary search-button-settings\"\n          type=\"submit\"\n        >\n          Try searching again\n        </Button>\n      </div>\n    </div>\n  );\n};\n\nexport default Searchresult;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchresultitem.js",["177","178"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Mainco
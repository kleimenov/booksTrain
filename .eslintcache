sFromDatabase();\n  }, []);\n\n  //it is test of connection with server\n  /*\n  const getAllUsersFromDatabase = async () => {\n    const response = await fetch(\"http://localhost:3002\");\n    const data = await response.json();\n    //console.log(data)\n  };\n*/\n\n\n  if (!token) {\n    return (\n      <Router>\n        <div className=\"App\">\n          <div className=\"header\">\n            <Header token={token} setToken={setToken} />\n          </div>\n          <div className=\"contianer\">\n            <Switch>\n              <Route path=\"/\" exact>\n                <Homepage token={token} setToken={setToken} />\n              </Route>\n              <Route\n                path=\"/loginform\"\n                render={(props) => (\n                  <Loginform\n                    {...props}\n                    setToken={setToken}\n                    setUserData={setUserData}\n                  />\n                )}\n              />\n              <Route path=\"/registrationform\" component={Registrationform} />\n            </Switch>\n          </div>\n          <div className=\"footer\">\n            <Footer />\n          </div>\n        </div>\n      </Router>\n    );\n  }\n  return (\n    <Router>\n      <Redirect to=\"/\" />\n      <div className=\"App\">\n        <div className=\"header\">\n          <Header token={token} />\n        </div>\n        <div className=\"contianer\">\n          <Switch>\n            <Route path=\"/\" exact>\n              <Homepage\n                key={userData[0].id}\n                userName={userData[0].first_name}\n                token={token}\n              />\n            </Route>\n            <Route path=\"/mybooks\">\n              <Mybooks\n                component={Mybooks}\n                myBooks={myBooks}\n                setMyBooks={setMyBooks}\n                userData={userData[0]}\n              />\n            </Route>\n          </Switch>\n        </div>\n        <div className=\"footer\">\n          <Footer />\n        </div>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Header.js",[],["132","133"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/FooterComponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Header.js",["134"],"import React, { useState } from \"react\";\nimport Nav from \"./Nav\";\nimport Authentication from \"./Authentification\";\nimport Logout from \"./Logout\";\nimport { Link } from \"react-router-dom\";\n\nconst Header = ({ token, setToken }) => {\n\n  if (!token) {\n    return (\n      <div className=\"NavWrapper\">\n        <Link to=\"/\">\n          <h1 className=\"custom-logo-font\">booksTrain</h1>\n        </Link>\n        <Authentication />\n      </div>\n    );\n  }\n  return (\n    <div className=\"NavWrapper\">\n      <Link to=\"/\">\n        <h1 className=\"custom-logo-font\">booksTrain</h1>\n      </Link>\n      <Nav />\n      <Logout token={ token } setToken={ setToken }/>\n    </div>\n  );\n};\n\nexport default Header;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Footercomponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/MainComponents/User.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/MainComponents/LoginForm.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/User.js",[],["135","136"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Loginform.js",["137","138","139","140"],"import React, { useState, useEffect } from \"react\";\nimport PropTypes from \"prop-types\";\nimport { Redirect, useHistory } from \"react-router-dom\";\n\nasync function loginUser(credentials) {\n  return fetch(\"http://localhost:3002/login\", {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(credentials),\n  }).then((response) => response.json());\n}\n\nconst Loginform = ({ setToken, setUserData, history }) => {\n  const [userEmail, setEmail] = useState();\n  const [userPass, setPassword] = useState();\n\n  const handleSubmit = async (evt) => {\n    evt.preventDefault();\n\n    const data = await loginUser({\n      userEmail: userEmail,\n      userPass: userPass,\n    });\n    //console.log(data)\n    setUserData(data.userData);\n    setToken(data.user);\n  };\n\n  return (\n    <div className=\"container\">\n      <h1 className=\"text-secondary\">Login form page</h1>\n      <div className=\"d-flex flex-column\">\n        <form onSubmit={handleSubmit}>\n          <label\n            className=\"text-secondary mt-2 login-text-holder\"\n            htmlFor=\"email\"\n          >\n            Please enter your email\n          </label>\n          <input\n            className=\"form-control ml-0 my-1\"\n            type=\"email\"\n            name=\"email\"\n            required\n            autoFocus\n            onChange={(evt) => setEmail(evt.target.value)}\n          ></input>\n          <label\n            className=\"text-secondary mt-2 login-text-holder\"\n            htmlFor=\"password\"\n          >\n            Please enter your password\n          </label>\n          <input\n            className=\"form-control ml-0 my-1\"\n            type=\"password\"\n            name=\"password\"\n            required\n            autoFocus\n            onChange={(evt) => setPassword(evt.target.value)}\n          ></input>\n          <button\n            type=\"submit\"\n            className=\"registration-submit-button text-white my-2 pb-1\"\n          >\n            Login\n          </button>\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default Loginform;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Homepage.js",["141","142"],"import React, { useState, useEffect } from \"react\";\nimport { Button } from \"react-bootstrap\";\nimport { Carousel } from \"react-responsive-carousel\";\nimport \"react-responsive-carousel/lib/styles/carousel.min.css\";\n//import axios from \"axios\";\nimport Books from \"./Books\";\nimport Searchengine from \"./Searchengine\";\nimport News from \"./Newshomepage\";\nimport Clock from \"./Clock\";\nimport Searchresult from \"./Searchresult\";\n\nconst Homepage = (props) => {\n  const [books, setBookslist] = useState([]);\n  const [news, setNews] = useState([]);\n  const [searchResponse, setResponse] = useState(false);\n\n  const url = \"\";\n  const url2 = \"https://www.googleapis.com/books/v1/volumes?q=Orwell\";\n\n  useEffect(() => {\n    getBooksList();\n    getAllNewsFromDatabase();\n  }, []);\n\n  const getBooksList = async () => {\n    const response = await fetch(url2);\n    const data = await response.json();\n    setBookslist(data.items);\n  };\n\n  /*\n  // Just in case if I would like to use axios\n  const getBooksList =  async () => {\n    axios.request(url).then((response) => {\n      setBookslist(response.data.items);\n      //response.data.items.map((item)=> {console.log(item)})\n    }).catch((error) => {\n      console.error(error);\n    })\n  };\n */\n\n  const getAllNewsFromDatabase = async () => {\n    const response = await fetch(\"http://localhost:3002/news\");\n    const data = await response.json();\n    setNews(data);\n  };\n\n  const returnHandler = () => {\n    setResponse(false);\n  };\n\n  if (!props.token) {\n    return (\n      <div>\n        <div className=\"d-flex flex-row justify-content-around align-items-center py-2\">\n          <h3 className=\"text-center text-secondary\">\n            Most popular for today!\n          </h3>\n          <Clock date={new Date()} />\n        </div>\n        <Carousel showThumbs={false} /*autoPlay*/>\n          {books.splice(0, 5).map((book) => (\n            <Books\n              key={book.id}\n              image={\n                book.volumeInfo.imageLinks\n                  ? book.volumeInfo.imageLinks.smallThumbnail\n                  : \"\"\n              }\n              title={book.volumeInfo.title}\n              author={book.volumeInfo.authors}\n              desc={book.volumeInfo.publishedDate}\n            />\n          ))}\n        </Carousel>\n        <div>\n          <h3 className=\"text-center text-secondary mt-4\">Library news!</h3>\n          <ul className=\"newscontainer py-2\">\n            {news.map((artical) => (\n              <News key={artical.news_id} news={artical.news_text} />\n            ))}\n          </ul>\n        </div>\n      </div>\n    );\n  }\n  if (searchResponse == []) {\n    return (\n      <div>\n        <h3 className=\"text-center text-secondary py-2\">\n          Welcome {props.userName}, let's find right book for you!\n        </h3>\n        <Searchengine\n          {...props}\n          searchResponse={searchResponse}\n          setResponse={setResponse}\n        />\n      </div>\n    );\n  }\n  if (searchResponse.length > 0) {\n    return (\n      <div>\n        <h4 className=\"text-center text-secondary py-2\">\n          {props.userName}, this is result of your search...\n        </h4>\n        <Searchresult searchResult={searchResponse} setResponse={setResponse} />\n      </div>\n    );\n  } else {\n    return (\n      <div>\n        <h4 className=\"text-center text-secondary py-2\">\n          {props.userName}, Sorry I didn't find anything..\n        </h4>\n        <Button\n          onClick={returnHandler}\n          className=\"btn-secondary search-button-settings\"\n          type=\"submit\"\n        >\n          Try searching again\n        </Button>\n      </div>\n    );\n  }\n};\n\nexport default Homepage;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Registrationform.js",["143"],"import React from 'react'\nimport axios from 'axios'\n\nconst Registrationform = () => (\n    <div className=\"container\">\n        <h1 className=\"text-secondary\">Registration form page </h1>\n        <div className=\"d-flex flex-column\">\n            <form action=\"\">\n               \n                <label className=\"text-secondary mt-2 login-text-holder\" htmlFor=\"first_name\">Please enter your first name</label>\n                <input className=\"form-control ml-0 my-1\" type=\"first_name\" name=\"first_name\" required autoFocus></input>\n                <label className=\"text-secondary mt-2 login-text-holder\" htmlFor=\"last_name\">Please enter your last name</label>\n                <input className=\"form-control ml-0 my-1\" type=\"last_name\" name=\"last_name\" required autoFocus></input>\n                <label className=\"text-secondary mt-2 login-text-holder\" htmlFor=\"phone_number\">Please enter your phone number</label>\n                <input className=\"form-control ml-0 my-1\" type=\"phone_name\" name=\"phone_number\" required autoFocus></input>\n                <label className=\"text-secondary mt-2 login-text-holder\" htmlFor=\"email\">Please enter your email</label>\n                <input className=\"form-control ml-0 my-1\" type=\"email\" name=\"email\" required autoFocus></input>\n                <label className=\"text-secondary mt-2 login-text-holder\" htmlFor=\"password\">Please enter your password</label>\n                <input className=\"form-control ml-0 my-1\" type=\"password\" name=\"password\" required autoFocus></input>\n                <button type=\"submit\" className=\"registration-submit-button text-white my-2 pb-1\">Submit</button>\n            </form>\n        </div>\n    </div>\n)\n\nexport default Registrationform","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/test.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Testcomp.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Logout.js",["144","145","146"],"import React, { useState, useEffect } from \"react\";\nimport { Redirect, Link } from \"react-router-dom\";\n\nconst Logout = ({ token, setToken }) => {\n  const logoutHandler = () => {\n    localStorage.clear();\n    document.location.href = \"/\";\n  };\n\n  return (\n    <div>\n      <Link to=\"/\">\n        <button\n          onClick={logoutHandler}\n          className=\"btn signIn-submit-button\"\n        >\n          Log Out\n        </button>\n      </Link>\n    </div>\n  );\n};\n\n\nexport default Logout;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Books.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Newshomepage.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchengine.js",["147"],"import React, { useEffect, useState } from \"react\";\nimport { Form, Card, Button } from \"react-bootstrap\";\n\nasync function searchRequest(credentials) {\n  return fetch(\"http://localhost:3002/booksSearch\", {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(credentials),\n  }).then((response) => response.json());\n}\n\nconst Searchengine = ({ setResponse }) => {\n  useEffect(() => {\n    searchRequest();\n  }, []);\n\n  const [Author, setAuthor] = useState();\n  const [Genre, setGenre] = useState();\n  const [Country, setCountry] = useState();\n  const [BookName, setBookName] = useState();\n  //const [searchResponse, setResponse] = useState([]);\n\n  const toLowerCase = (variable) => {\n    for (let item in variable) {\n      if (variable[item]) {\n        variable[item] = variable[item].toLowerCase();\n      }\n    }\n  };\n\n  const data = {\n    searchAuthor: Author,\n    searchGenre: Genre,\n    searchCountry: Country,\n    searchBookName: BookName,\n  };\n\n  toLowerCase(data);\n\n  const checkEmptyInputs = (data, passPermit) => {\n    let cnt = 0;\n    for (let item in data) {\n      if (!data[item] || data[item] == \"\") {\n        cnt++;\n      }\n    }\n    return cnt > 3 ? (passPermit = false) : (passPermit = true);\n  };\n\n  const searchHandler = async (evt) => {\n    const requestPermit = false;\n    evt.preventDefault();\n\n    if (checkEmptyInputs(data, requestPermit)) {\n      const serverResponse = await searchRequest(data);\n      setResponse(serverResponse);\n    } else {\n      alert(\"Please fill out at least one input line (whatever you want)\");\n    }\n  };\n\n  return (\n    <Card className=\"text-left\">\n      <Card.Body>\n        <Form\n          className=\"text-secondary search-form-settings\"\n          onSubmit={searchHandler}\n        >\n          <Form.Group id=\"Author\">\n            <Form.Label>Search by Author</Form.Label>\n            <Form.Control\n              type=\"Author\"\n              onChange={(evt) => setAuthor(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Genre\">\n            <Form.Label>Search by Genre</Form.Label>\n            <Form.Control\n              type=\"Genre\"\n              onChange={(evt) => setGenre(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Country\">\n            <Form.Label>Search by Country</Form.Label>\n            <Form.Control\n              type=\"Country\"\n              onChange={(evt) => setCountry(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"bookName\">\n            <Form.Label>Search by bo
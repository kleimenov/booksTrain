);\n  const [BookName, setBookName] = useState();\n  //const [searchResponse, setResponse] = useState([]);\n\n  const toLowerCase = (variable) => {\n    for (let item in variable) {\n      if (variable[item]) {\n        variable[item] = variable[item].toLowerCase();\n      }\n    }\n  };\n\n  const data = {\n    searchAuthor: Author,\n    searchGenre: Genre,\n    searchCountry: Country,\n    searchBookName: BookName,\n  };\n\n  toLowerCase(data);\n\n  const checkEmptyInputs = (data, passPermit) => {\n    let cnt = 0;\n    for (let item in data) {\n      if (!data[item] || data[item] == \"\") {\n        cnt++;\n      }\n    }\n    return cnt > 3 ? (passPermit = false) : (passPermit = true);\n  };\n\n  const searchHandler = async (evt) => {\n    const requestPermit = false;\n    evt.preventDefault();\n\n    if (checkEmptyInputs(data, requestPermit)) {\n      const serverResponse = await searchRequest(data);\n      setResponse(serverResponse);\n    } else {\n      alert(\"Please fill out at least one input line (whatever you want)\");\n    }\n  };\n\n  return (\n    <Card className=\"text-left\">\n      <Card.Body>\n        <Form\n          className=\"text-secondary search-form-settings\"\n          onSubmit={searchHandler}\n        >\n          <Form.Group id=\"Author\">\n            <Form.Label>Search by Author</Form.Label>\n            <Form.Control\n              type=\"Author\"\n              onChange={(evt) => setAuthor(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Genre\">\n            <Form.Label>Search by Genre</Form.Label>\n            <Form.Control\n              type=\"Genre\"\n              onChange={(evt) => setGenre(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Country\">\n            <Form.Label>Search by Country</Form.Label>\n            <Form.Control\n              type=\"Country\"\n              onChange={(evt) => setCountry(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"bookName\">\n            <Form.Label>Search by book's name</Form.Label>\n            <Form.Control\n              type=\"bookName\"\n              onChange={(evt) => setBookName(evt.target.value)}\n            />\n          </Form.Group>\n          <Button\n            className=\"w-50 btn-secondary search-button-settings\"\n            type=\"submit\"\n          >\n            Search\n          </Button>\n        </Form>\n      </Card.Body>\n    </Card>\n  );\n};\n\nexport default Searchengine;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Mybooks.js",["165","166"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Clock.js",["167"],"import React, { useState, useEffect } from \"react\";\n\n/*\nclass Clock extends React.Component {\n    constructor(props) {\n      super(props);\n      this.state = {date: new Date()};\n    }\n  \n    componentDidMount() {\n      this.timerID = setInterval(\n        () => this.tick(),\n        1000\n      );\n    }\n  \n    componentWillUnmount() {\n      clearInterval(this.timerID);\n    }\n  \n    tick() {\n      this.setState({\n        date: new Date()\n      });\n    }\n  \n    render() {\n      return (\n        <div className=\"text-center text-secondary pt-1\">\n          <h5>{this.state.date.toLocaleTimeString([], { hour: '2-digit', minute: '2-digit' })}</h5>\n        </div>\n      );\n    }\n  }\n*/\n\nconst Clock = () => {\n  const [currentTime, setCurrentTime] = useState(\n    new Date().toLocaleTimeString()\n  );\n\n  useEffect(() => {\n    const timer = setTimeout(() => {\n      setCurrentTime(new Date().toLocaleTimeString());\n    }, 1000);\n  });\n  return (\n    <div className=\"text-center text-secondary pt-1\">\n      <h6>{currentTime}</h6>\n    </div>\n  );\n};\n\nexport default Clock;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchresult.js",["1
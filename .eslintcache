rain/src/components/HeaderComponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/FooterComponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Header.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Footercomponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/MainComponents/User.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/MainComponents/LoginForm.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/User.js",[],["164","165"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Loginform.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Homepage.js",["166"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Registrationform.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/test.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Testcomp.js",[],["167","168"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Logout.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Books.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Newshomepage.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchengine.js",["169"],"import React, { useEffect, useState } from \"react\";\nimport { Form, Card, Button } from \"react-bootstrap\";\n\nasync function searchRequest(credentials) {\n  return fetch(\"http://localhost:3002/booksSearch\", {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(credentials),\n  }).then((response) => response.json());\n}\n\nconst Searchengine = ({ setResponse, userId }) => {\n  useEffect(() => {\n    searchRequest();\n  }, []);\n\n  const [Author, setAuthor] = useState();\n  const [Genre, setGenre] = useState();\n  const [Country, setCountry] = useState();\n  const [BookName, setBookName] = useState();\n\n  const toLowerCase = (variable) => {\n    for (let item in variable) {\n      if (variable[item]) {\n        variable[item] = variable[item].toLowerCase();\n      }\n    }\n  };\n\n  const data = {\n    searchAuthor: Author,\n    searchGenre: Genre,\n    searchCountry: Country,\n    searchBookName: BookName,\n    userId: userId.toString(),\n  };\n\n  toLowerCase(data);\n\n  const checkEmptyInputs = (data, passPermit) => {\n    let cnt = 0;\n    for (let item in data) {\n      if (!data[item] || data[item] == \"\") {\n        cnt++;\n      }\n    }\n    return cnt > 3 ? (passPermit = false) : (passPermit = true);\n  };\n\n  const searchHandler = async (evt) => {\n    const requestPermit = false;\n    evt.preventDefault();\n\n    if (checkEmptyInputs(data, requestPermit)) {\n      const serverResponse = await searchRequest(data);\n      setResponse(serverResponse);\n    } else {\n      alert(\"Please fill out at least one input line (whatever you want)\");\n    }\n  };\n\n  return (\n    <Card className=\"text-left\">\n      <Card.Body>\n        <Form\n          className=\"text-secondary search-form-settings\"\n          onSubmit={searchHandler}\n        >\n          <Form.Group id=\"Author\">\n            <Form.Label>Search by Author</Form.Label>\n            <Form.Control\n              type=\"Author\"\n              onChange={(evt) => setAuthor(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Genre\">\n            <Form.Label>Search by Genre</Form.Label>\n            <Form.Control\n              type=\"Genre\"\n              onChange={(evt) => setGenre(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"Country\">\n            <Form.Label>Search by Country</Form.Label>\n            <Form.Control\n              type=\"Country\"\n              onChange={(evt) => setCountry(evt.target.value)}\n            />\n          </Form.Group>\n          <Form.Group id=\"bookName\">\n            <Form.Label>Search by book's name</Form.Label>\n            <Form.Control\n              type=\"bookName\"\n              onChange={(evt) => setBookName(evt.target.value)}\n            />\n          </Form.Group>\n          <Button\n            className=\"w-50 btn-secondary search-button-settings\"\n            type=\"submit\"\n          >\n            Search\n          </Button>\n        </Form>\n      </Card.Body>\n    </Card>\n  );\n};\n\nexport default Searchengine;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Mybooks.js",["170"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Clock.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchresult.js",["171"],"import React from \"react\";\nimport Searchresultitem from \"./Searchresultitem\";\nimport { Button } from \"react-bootstrap\";\n\n\nconst Searchresult = ({ searchResult, setResponse, userName, userId }) => {\n  const returnHandler = () => {\n    setResponse(false);\n  };\n\n  const capitalizaResult = (data) => {\n    const titleCase = (string) => {\n      let splitStr = string.split(\" \");\n      let newArr = [];\n      for (let word of splitStr) {\n        word = word.charAt(0).toUpperCase() + word.slice(1);\n        newArr.push(word);\n      }\n      return newArr.join(\" \");\n    };\n\n    data.map((item) => {\n      let tmp = item.book_name.split(\" \");\n      tmp[0] = tmp[0][0].toUpperCase() + tmp[0].slice(1);\n      item.book_name = tmp.join(\" \");\n\n      item.author = titleCase(item.author);\n    });\n  };\n\n  capitalizaResult(searchResult);\n\n  return (\n    <div>\n      <ul className=\"list-unstyled\">\n        {searchResult.map((item) => (\n          <Searchresultitem\n            key={item.book_id}\n            bookName={item.book_name}\n            bookAuthor={item.author}\n            userName={userName}\n            userId={userId}\n            bookId={item.book_id}\n            bookStatus={item.user_id}\n            wishStatus={item.isInWishlist}\n          />\n        ))}\n      </ul>\n      <div className=\"text-right\">\n        <Button\n          onClick={returnHandler}\n          className=\"btn-secondary search-button-settings\"\n          type=\"submit\"\n        >\n          Try searching again\n        </Button>\n      </div>\n    </div>\n  );\n};\n\nexport default Searchresult;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchresultitem.js",["172","173"],"import React, { useState } from \"react\";\nimport { Button } from \"react-bootstrap\";\n\nconst Searchresultitem = ({\n  bookName,\n  bookAuthor,\n  userName,\n  bookId,\n  userId,\n  bookStatus,\n  wishStatus\n}) => {\n\n  const [status, setStatus] = useState(bookStatus ? true : false);\n  const [whoRead, setReader] = useState(bookStatus);\n  const [userWish, setUserWish] = useState(wishStatus)\n\n  const addBook = async (credientials) => {\n    const response = await fetch(\"http://localhost:3002/addbook\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(credientials),\n    });\n    const data = await response.json();\n  };\n\n  const addReadingList = () => {\n    if (!status) {\n      addBook({ \n        userId: userId,\n        bookId: bookId,\n        userName: userName,\n        unixTime: new Date().getTime(),\n      });\n      setStatus(!status);\n      setReader(userId);\n    }\n    console.log(\"This book is already added\");\n  };\n\n  const addBookWishList = async (credientials) => {\n    const response = await fetch(\"http://localhost:3002/wishlist\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(credientials),\n    });\n    const data = await response.json();\n  };\n\n  \n  const addWishList = () => {\n    addBookWishList({\n      userId: userId,\n      bookId: bookId,\n      unixTime: new Date().getTime(),\n    });\n    setUserWish(!userWish)\n  };\n\n  if (!status) {\n    return (\n      <li className=\"my-2\">\n        <div className=\"search-wrapper\">\n          <h5 className=\"text-secondary ml-2 pt-2\">\n            {bookName} / {bookAuthor}{\" \"}\n          </h5>\n          <Button\n            onClick={addReadingList}\n            className=\"btn-success p-0 px-1 py-0 ml-1\"\n            type=\"submit\"\n          >\n            +book\n          </Button>\n        </div>\n      </li>\n    );\n  }\n  if (userId === whoRead) {\n    return (\n      <li className=\"my-2\">\n        <div className=\"search-wrapper\">\n          <h5 className=\"text-secondary ml-2 pt-2\">\n            {bookName} / {bookAuthor} / in your reading list\n          </h5>\n        </div>\n      </li>\n    );\n  }\n  if (userWish) {\n    return (\n      <li className=\"my-2\">\n        <div className=\"search-wrapper\">\n          <h5 className=\"text-secondary ml-2 pt-2\">\n            {bookName} / {bookAuthor} / in your wish list\n          </h5>\n        </div>\n      </li>\n    );\n  }\n  return (\n    <li className=\"my-2\">\n      <div className=\"search-wrapper\">\n        <h5 className=\"text-secondary ml-2 pt-2\">\n          {bookName} / {bookAuthor}{\" \"}\n        </h5>\n        <Button\n          onClick={addWishList}\n  
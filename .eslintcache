en) {\n    return (\n      <div>\n        <div className=\"d-flex flex-row justify-content-around align-items-center py-2\">\n          <h3 className=\"text-center text-secondary\">\n            Most popular for today!\n          </h3>\n          <Clock date={new Date()} />\n        </div>\n        <Carousel showThumbs={false} /*autoPlay*/>\n          {books.splice(0, 5).map((book) => (\n            <Books\n              key={book.id}\n              image={\n                book.volumeInfo.imageLinks\n                  ? book.volumeInfo.imageLinks.smallThumbnail\n                  : \"\"\n              }\n              title={book.volumeInfo.title}\n              author={book.volumeInfo.authors}\n              desc={book.volumeInfo.publishedDate}\n            />\n          ))}\n        </Carousel>\n        <div>\n          <h3 className=\"text-center text-secondary mt-4\">Library news!</h3>\n          <ul className=\"newscontainer py-2\">\n            {news.map((artical) => (\n              <News key={artical.news_id} news={artical.news_text} />\n            ))}\n          </ul>\n        </div>\n      </div>\n    );\n  }\n  if (searchResponse == []) {\n    return (\n      <div>\n        <h3 className=\"text-center text-secondary py-2\">\n          Welcome {props.userName}, let's find right book for you!\n        </h3>\n        <Searchengine\n          {...props}\n          searchResponse={searchResponse}\n          setResponse={setResponse}\n        />\n      </div>\n    );\n  }\n  if (searchResponse.length > 0) {\n    return (\n      <div>\n        <h4 className=\"text-center text-secondary py-2\">\n          {props.userName}, this is result of your search...\n        </h4>\n        <Searchresult\n          searchResult={searchResponse}\n          setResponse={setResponse}\n          userName={props.userName}\n          userId={props.userId}\n        />\n      </div>\n    );\n  } else {\n    return (\n      <div>\n        <h4 className=\"text-center text-secondary py-2\">\n          {props.userName}, Sorry I didn't find anything..\n        </h4>\n        <Button\n          onClick={returnHandler}\n          className=\"btn-secondary search-button-settings\"\n          type=\"submit\"\n        >\n          Try searching again\n        </Button>\n      </div>\n    );\n  }\n};\n\nexport default Homepage;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Registrationform.js",["158"],"import React from \"react\";\nimport axios from \"axios\";\n\nconst Registrationform = () => (\n  <div className=\"container\">\n    <h1 className=\"text-secondary\">Registration form page </h1>\n    <div className=\"d-flex flex-column\">\n      <form action=\"\">\n        <label\n          className=\"text-secondary mt-2 login-text-holder\"\n          htmlFor=\"first_name\"\n        >\n          Please enter your first name\n        </label>\n        <input\n          className=\"form-control ml-0 my-1\"\n          type=\"first_name\"\n          name=\"first_name\"\n          required\n          autoFocus\n        ></input>\n        <label\n          className=\"text-secondary mt-2 login-text-holder\"\n          htmlFor=\"last_name\"\n        >\n          Please enter your last name\n        </label>\n        <input\n          className=\"form-control ml-0 my-1\"\n          type=\"last_name\"\n          name=\"last_name\"\n          required\n          autoFocus\n        ></input>\n        <label\n          className=\"text-secondary mt-2 login-text-holder\"\n          htmlFor=\"phone_number\"\n        >\n          Please enter your phone number\n        </label>\n        <input\n          className=\"form-control ml-0 my-1\"\n          type=\"phone_name\"\n          name=\"phone_number\"\n          required\n          autoFocus\n        ></input>\n        <label\n          className=\"text-secondary mt-2 login-text-holder\"\n          htmlFor=\"email\"\n        >\n          Please enter your email\n        </label>\n        <input\n          className=\"form-control ml-0 my-1\"\n          type=\"email\"\n          name=\"email\"\n          required\n          autoFocus\n        ></input>\n        <label\n          className=\"text-secondary mt-2 login-text-holder\"\n          htmlFor=\"password\"\n        >\n          Please enter your password\n        </label>\n        <input\n          className=\"form-control ml-0 my-1\"\n          type=\"password\"\n          name=\"password\"\n          required\n          autoFocus\n        ></input>\n        <button\n          type=\"submit\"\n          className=\"registration-submit-button text-white my-2 pb-1\"\n        >\n          Submit\n        </button>\n      </form>\n    </div>\n  </div>\n);\n\nexport default Registrationform;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/test.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Testcomp.js",[],["159","160"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Logout.js",["161","162","163"],"import React, { useState, useEffect } from \"react\";\nimport { Redirect, Link } from \"react-router-dom\";\n\nconst Logout = ({ token, setToken }) => {\n  const logoutHandler = () => {\n    localStorage.clear();\n    document.location.href = \"/\";\n  };\n\n  return (\n    <div>\n      <Link to=\"/\">\n        <button\n          onClick={logoutHandler}\n          className=\"btn signIn-submit-button\"\n        >\n          Log Out\n        </button>\n      </Link>\n    </div>\n  );\n};\n\nexport default Logout;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Books.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Newshomepage.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Maincomponents/Searchengine.js",["164"],"import React, { useEffect, useState } from \"react\";\nimport { Form, Card, Button } from \"react-bootstrap\";\n\nasync function searchRequest(credentials) {\n  return fetch(\"http://localhost:3002/booksSearch\", {\n    method: \"POST\",\n    headers: {\n      \"Content-Type\": \"application/json\",\n    },\n    body: JSON.stringify(credentials),\n  }).then((response) => response.json());\n}\n\nconst Searchengine = ({ setResponse }) => {\n  useEffect(() => {\n    searchRequest();\n  }, []);\n\n  const [Author, setAuthor] = useState();\n  const [Genre, setGenre] = useState();\n  const [Country, setCountry] = useState();\n  const [BookName, setBookName] = useState();\n  //const [searchResponse, setResponse] = useState([]);\n\n  const toLowerCase = (variable) => {\n    for (let item in variable) {\n      if (variable[item]) {\n        variable[item] = variable[item].toLowerCase();\n      }\n    }\n  };\n\n  const data = {\n    searchAuthor: Author,\n    searchGenre: Genre,\n    searchCountry: Country,\n    searchBookName: BookName,\n  };\n\n  toLowerCase(data);\n\n  const checkEmptyInputs = (data, passPermit) => {\n    let cnt = 0;\n    for (let item in data) {\n      if (!data[item] || data[item] == \"\") {\n        cnt++;\n      }\n    }\n    return cnt > 3 ? (passPermit = false) : (passPermit = true);\n  };\n\n  const searchHandler = async (evt) => {\n    const requestPermit = false;\n    evt.preventDefault();\n\n    if (checkEmptyInputs(data, requestPermit)) {\n      const serverResponse = await searchRequest(data);\n      setResponse(serverResponse);\n    } else {\n      alert(\"Please fill out at least one input line (whatever you want)\");\n    }\n  };\n\n  return (\n    <Card className=\"text-left\">\n      <Card.Body>\n        <Form\n          className=\"text-secondary search-form-settings\"\n          onSubmit={searchHandler}\n        >\n          <Form.Group id=\"Author\">\n            <Form.Label>Search by Author</Form.Label>\n            <Form.Control\n              type=\"Author\"\n              onChange={(evt) => setAu
\n                )}\n              />\n              <Route path=\"/registrationform\" component={Registrationform} />\n            </Switch>\n          </div>\n          <div className=\"footer\">\n            <Footer />\n          </div>\n        </div>\n      </Router>\n    );\n  }\n  return (\n    <Router>\n      <Redirect to=\"/\" />\n      <div className=\"App\">\n        <div className=\"header\">\n          <Header token={token} />\n        </div>\n        <div className=\"contianer\">\n          <Switch>\n            <Route path=\"/\" exact>\n              <Homepage\n                key={userData[0].id}\n                userName={userData[0].first_name}\n                token={token}\n                userId={userData[0].id}\n              />\n            </Route>\n            <Route path=\"/mybooks\">\n              <Mybooks component={Mybooks} userData={userData[0]} />\n            </Route>\n            <Route path=\"/wishlist\">\n              <Wishlist />\n            </Route>\n          </Switch>\n        </div>\n        <div className=\"footer\">\n          <Footer />\n        </div>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Header.js",[],["150","151"],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Nav.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/HeaderComponents/Authentification.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/FooterComponents/Footer.js",[],"/Users/deelooc21/Desktop/Developer/Projects/bookstrain/src/components/Headercomponents/Header.js",["152"],"import React, { useState } from \"react\";\nimport Nav from \"./Nav\";\nimport Authentication from \"./Authentification\";\nimport Logout from \"./Logout\";\nimport { Link } from \"react-router-dom\";\n\nconst Header = ({ token, setToken }) => {\n\n  if (!token) {\n    return (\n   